{
  "require-dev": {
    "symfony/var-dumper": "^7.3"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/helpers.php",
      "config.php"
    ]
  },
  "scripts": {
    "dev": [
      "Composer\\Config::disableProcessTimeout",
      "npx concurrently -c \"#ff4242,#c4b5fd\" \"php dev.php\" \"npm run dev\" --names=server,vite"
    ],
    "build": [
      "Composer\\Config::disableProcessTimeout",
      "php build.php"
    ]
  }
}
